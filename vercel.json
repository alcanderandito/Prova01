{
    "functions": {
        "api/send-message.js": {
            "memory": 128,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/send-message",
            "dest": "/api/send-message.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 